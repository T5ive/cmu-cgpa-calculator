{
    "name": "RegCMU Cumulative GPA Calculator",
    "description": "Google Chrome Extension for predict and calculate your cumulative GPA (CGPA) in the transcript page of Reg CMU",
    "version": "1",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": [
                "https://www3.reg.cmu.ac.th/transcript/result.php?time=*"
            ],
            "js": [
                "thirdParty/jquery-3.2.1.min.js",
                "cgpa-calculator.js"
            ]
        }
    ],
    "web_accessible_resources": [
        "table-template.html",
        "images/wait-bottom-bg2.gif",
        "images/wait-bottom-left2.gif",
        "images/wait-bottom-right2.gif",
        "images/wait-no-bg.gif",
        "images/wait-no-left.gif",
        "images/wait-no-right.gif",
        "images/wait-top-bg.gif",
        "images/wait-top-left.gif",
        "images/wait-top-right.gif"

    ],
    "background":
    {
        "scripts": ["thirdParty/jquery-3.2.1.min.js"]
    }
}